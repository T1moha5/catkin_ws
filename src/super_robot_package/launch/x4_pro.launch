<launch>
  <node name="ydlidar_lidar_publisher" pkg="ydlidar_ros_driver" type="ydlidar_ros_driver_node" output="screen">
    <param name="port" type="string" value="/dev/ydlidar"/>  
    <param name="frame_id" type="string" value="laser_frame"/>
    <param name="baudrate" type="int" value="128000"/>  
    <param name="lidar_type" type="int" value="1"/>  
    <param name="device_type" type="int" value="0"/>  
    <param name="sample_rate" type="int" value="5"/>  
    <param name="auto_reconnect" type="bool" value="true"/>
    <param name="range_min" type="double" value="0.1" />
    <param name="range_max" type="double" value="12.0" />
    <param name="frequency" type="double" value="8.0"/>  
    <param name="resolution_fixed" type="bool" value="false"/> 

  </node>

  <node pkg="tf" type="static_transform_publisher" name="base_link_to_laser4"
        args="0.0 0.0 0.2 0.0 0.0 0.0 base_footprint laser_frame 40"/>
</launch>


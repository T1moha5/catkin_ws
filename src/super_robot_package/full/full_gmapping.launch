<?xml version="1.0"?>
<launch>

     <include file="$(find super_robot_package)/full_rs_odom.launch"/>

     <include file="$(find super_robot_package)/drivers/tf_transforms.launch"/>

     <include file="$(find super_robot_package)/drivers/slam_gmapping.launch">
     <param name="fixed_frame" value = "odom"/>                                                                 
     <param name="use_odom" value="true"/>                                                                      
